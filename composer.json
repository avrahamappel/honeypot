{
    "name": "appel/honeypot",
    "description": "A simple spam protection for Laravel 5.5.",
    "keywords": [
        "laravel",
        "spam",
        "forms",
        "honeypot",
        "honeytime",
        "appel",
        "bomshteyn"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Avraham Appel",
            "email": "avraham@bomshteyn.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^5|^6",
        "illuminate/config": "^5|^6",
        "illuminate/translation": "^5|^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Appel\\Honeypot\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appel\\Honeypot\\Providers\\HoneypotServiceProvider"
            ],
            "aliases": {
                "Honeypot": "Appel\\Honeypot\\Facades\\Honeypot"   
            }
        }
    }
}
